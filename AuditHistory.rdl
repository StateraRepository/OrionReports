    <DataSet Name="TableList">
      <Query>
        <DataSourceName>Hunter</DataSourceName>
        <CommandText>SELECT '-- ALL --' AS TableName, '%' AS TableValue
UNION
SELECT DISTINCT TableName, TableName AS TableValue
FROM     cda.ChangeDataConfig
ORDER BY TableName</CommandText>
      </Query>

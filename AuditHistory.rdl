  <DataSets>
    <DataSet Name="AuditHistory">
      <Query>
        <DataSourceName>Hunter</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TableName">
            <Value>=Parameters!TableName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ColumnName">
            <Value>=Parameters!ColumnName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 1000 [ChangeDataAuditId]
      ,[AuditDateTime]
      ,[OperationUser]
      ,[SchemaName]
      ,[TableName]
      ,[RowId]
      ,[OperationRef]
      ,[OperationType]
      ,[ColumnName]
      ,[OldValue]
      ,[NewValue]
  FROM [TheHunter].[cda].[ChangeDataAudit]
  where AuditDateTime &gt;= @StartDate
and AuditDateTime &lt;= @EndDate
and (TableName Like @TableName)
and ( ColumnName Like @ColumnName )
and (OperationUser Like @UserName)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

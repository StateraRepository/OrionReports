      <Query>
        <DataSourceName>TheHunter</DataSourceName>
        <CommandText>SELECT ClientName, CAST(ClientId AS varchar) + ' - ' + ClientName AS ClientIDAndName, ClientId
FROM     Client
WHERE  (LocationSampling = 1) AND (IsInactive = 0)
